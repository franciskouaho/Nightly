{
  "expo": {
    "name": "Nightly",
    "slug": "nightly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "cosmic-quest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emplica.nightly",
      "googleServicesFile": "./GoogleService-Info.plist",
      "appleTeamId": "NAN7MFNMT7",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#4b277d"
      },
      "versionCode": 2,
      "package": "com.emplica.nightly.android",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#4b277d"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3de41614-7f99-4215-bec0-9a2ece4bbd35"
      }
    },
    "owner": "iamfrancisco"
  }
}
