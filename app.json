{
  "expo": {
    "name": "Nightly",
    "slug": "nightly",
    "version": "1.7.0",
    "icon": "./assets/christmas/logo.png",
    "scheme": "nightly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emplica.nightly",
      "googleServicesFile": "./GoogleService-Info.plist",
      "appleTeamId": "NAN7MFNMT7",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": ["remote-notification"],
        "NSCameraUsageDescription": "Allow Nightly to access your camera to scan QR codes and join game rooms."
      },
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/christmas/logo.png",
        "backgroundColor": "#4b277d"
      },
      "package": "com.emplica.nightly.android",
      "googleServicesFile": "./google-services.json",
      "versionCode": 33,
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/christmas/logo.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "forceStaticLinking": ["RNFBApp", "RNFBAuth", "RNFBFirestore", "RNFBMessaging", "RNFBStorage"],
            "deploymentTarget": "15.1"
          },
          "android": {
            "permissions": ["com.android.vending.BILLING", "CAMERA"],
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enablePageSize16K": true,
            "enableEdgeToEdge": true,
            "statusBarStyle": "light",
            "statusBarBackgroundColor": "transparent"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/christmas/logo.png",
          "color": "#4b277d"
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Nightly to access your camera to scan QR codes and join game rooms."
        }
      ],
      "expo-font"
    ],
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3de41614-7f99-4215-bec0-9a2ece4bbd35"
      }
    },
    "owner": "iamfrancisco",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/3de41614-7f99-4215-bec0-9a2ece4bbd35",
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    },
    "runtimeVersion": "1.2.3"
  }
}
